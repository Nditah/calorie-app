{
  "name": "calorie-app",
  "version": "0.0.1",
  "description": "Afro Calorie App",
  "author": "Nditah Samweld <nditah@gmail.com>",
  "homepage": "https://calorie-app.herokuapp.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "sudo rm -rf node_modules && npm rebuild node-sass && npm i",
    "serve": "ionic serve --lab --debug",
    "ionic:android": "ionic cordova platform add android",
    "ionic:build:android": "cordova build android --prod --release  --stacktrace",
    "create:key": "keytool -genkey -v -keystore my-release-key.jks -keyalg RSA -keysize 2048 -validity 10000 -alias my-alias",
    "sign:app": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore my-release-key.jks  platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk my-alias",
    "zipalign:app": "cd ./platforms/android/app/build/outputs/apk/release && ~/Library/Android/sdk/build-tools/28.0.3/zipalign  -v 4 app-release-unsigned.apk AfroCalorie.apk",
    "deploy:apk": "npm run ionic:build:android && npm run sign:app",
    "verify:app": "npm run zipalign:app && cd ./platforms/android/app/build/outputs/apk/release && ~/Library/Android/sdk/build-tools/28.0.3/apksigner verify  AfroCalorie.apk",
    "move:apk": "mv ./platforms/android/app/build/outputs/apk/release/AfroCalorie.apk ~/Desktop"
  },
  "private": true,
