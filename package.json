{
    "name": "calorie-app",
    "version": "0.0.1",
    "description": "Afro Calorie App",
    "author": "Nditah Samweld <nditah@gmail.com>",
    "homepage": "https://calorie-app.herokuapp.com/",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "serve": "ionic cordova run browser",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "clean": "sudo rm -rf node_modules && npm rebuild node-sass && npm i",
        "lab": "ionic serve --lab",
        "apk": "cordova build android --prod --release  --stacktrace",
        "build:android": "ionic cordova build --release android",
        "build:browser": "ionic cordova build --release browser",
        "sign:app": "jarsigner -sigalg SHA1withRSA -digestalg SHA1 -keystore calorie-app.keystore platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk calorie-app && mv platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk calorie-app.apk",
        "create:key": "keytool -genkey -v -keystore calorie-app.keystore -alias calorie-app -keyalg RSA -keysize 2048 -validity 1000",
        "deploy:apk": "npm run build:android && npm run sign:app"
    },
    "private": true,
